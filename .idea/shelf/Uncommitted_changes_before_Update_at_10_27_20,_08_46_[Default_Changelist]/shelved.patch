ext-decoration:none;\n    padding:8px 30px;\n    color:white;\n    background-color:transparent;\n    border:1px solid white;\n    position:absolute;\n    top:50%;\n    left:50%;\n    transform:translate(-50%,-50%);\n}\n.creation-block a:nth-child(2):hover{\n    background-color:white;\n    color: #025d30;\n}\n.creation-block:hover a:nth-child(2){\n    display:inline-block;\n}\n.creation-block:hover img{\n    transform:scale(1.25);\n    filter:brightness(35%);\n}\n.creation-content > img{\n    position:absolute;\n    top:50%;\n    left:50%;\n    transform:translate(-50%,-50%);\n}\n.creation-content h3{\n    position:absolute;\n    margin:0;\n    color:white;\n    font-size:60px;\n    font-family: 'Montez', cursive;\n    top:50%;\n    left:50%;\n    transform:translate(-50%,-50%);\n}\n/*SUBSCRIBE CONTENT*/\n.subscribe-content{\n    padding:40px 0;\n}\n.subscribe-content h3{\n    font-size:23px;\n    font-weight:bold;\n    color:#0a8446;\n    text-align:center;\n    margin:0 0 20px;\n    padding-left:80px;\n}\n.icon-block span{\n    position:relative;\n    display:block;\n    margin:0 10px;\n    height:47px;\n    width:47px;\n    border-radius:100%;\n    background-color:#0a8446;\n}\n.icon-block span i{\n    position:absolute;\n    top:50%;\n    left:50%;\n    transform:translate(-50%,-50%);\n}\n.icon-block a{\n    float:right;\n    color:white;\n}\n.icon-block a:hover span{\n    background-color:#025d30;\n}\n.email-block input{\n    padding:9px;\n    margin:0 15px 0 10px;\n    width:75%;\n    border:1px solid #018445;\n}\n.email-block input::placeholder{\n    color:#018445;\n}\n.email-block form span{\n    display:inline-block;\n    border:2px solid #018445;\n}\n.email-block button {\n    color: white;\n    background-color:#018445;\n    border:1px solid white;\n    padding:7px 30px;\n}\n.email-block form span:hover button{\n    background-color:#025d30;\n}
